      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "esModuleInterop": true,
      "allowJs": true,
      "resolveJsonModule": true
